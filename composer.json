{
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "config": {
        "sort-packages": true,
        "vendor-dir": "app/vendor"
    },
    "require": {
        "erusev/parsedown": "^1.7",
        "laminas/laminas-db": "^2.11",
        "league/container": "^3.3",
        "scrivo/highlight.php": "v9.18.1.3",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.5",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "macfja/phar-builder": "^0.2.8",
        "symfony/var-dumper": "^5.1"
    },
    "extra": {
        "phar-builder": {
            "compression": "GZip",
            "name": "phpuush.phar",
            "output-dir": "./",
            "entry-point": "./index.php",
            "include-dev": false,
            "skip-shebang": true
        }
    }
}
